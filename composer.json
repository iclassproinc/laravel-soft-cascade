{
    "name": "byteasc/laravel-soft-cascade",
    "description": "Laravel Cascade Soft Delete & Restore",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremiah Fulbright",
            "email": "jeremiah@iclasspro.com",
            "homepage": "http://www.iclasspro.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/container": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/events": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Askedio\\SoftCascade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Askedio\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Askedio\\SoftCascade\\Providers\\GenericServiceProvider"
            ]
        }
    }
}